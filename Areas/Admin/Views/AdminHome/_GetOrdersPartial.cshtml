@using HamburgerMenuProject.Models.Entities;
@model List<Order>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Created Time
            </th>
            <th>
                Total Price
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (Order order in Model)
        {
            <tr>
                <td>
                    @order.ID
                </td>
                <td>
                    @order.CreatedTime
                </td>
                <td>
                    @order.Price
                </td>
            </tr>
        }
    </tbody>
</table>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}